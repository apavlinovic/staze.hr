{
    "name": "backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "./node_modules/.bin/tsc",
        "start": "./node_modules/.bin/tsc && node ./bin/www.js",
        "prettier:check": "./node_modules/.bin/prettier -c \"src/**/*.ts\"",
        "prettier:format": "./node_modules/.bin/prettier  \"src/**/*.ts\" --write",
        "dev": "./node_modules/.bin/tsc-watch --onSuccess \"node ./bin/www.js\""
    },
    "prettier": "./../.prettierrc.json",
    "dependencies": {
        "cookie-parser": "^1.4.5",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^8.5.1",
        "morgan": "~1.9.1",
        "node-cache": "^5.1.0",
        "pg": "^7.18.2",
        "pg-hstore": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.25"
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/express-serve-static-core": "^4.17.7",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.0.10",
        "@types/sequelize": "^4.28.9",
        "husky": "^4.2.5",
        "prettier": "^2.0.5",
        "ts-node": "^8.10.2",
        "tsc-watch": "^4.2.8",
        "typescript": "^3.9.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier:check"
        }
    }
}
